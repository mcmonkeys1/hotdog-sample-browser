{
  "expo": {
    "name": "hotdog-sample-browser",
    "slug": "hotdog-sample-browser",
    "description": "An interactive dApp that uses the arweave permaweb as storage for short, high quality audio samples.",
    "owner": "Ros McMahon rosmcmahon@gmail.com",
    "privacy": "public",
    "githubUrl": "https://github.com/mcmonkeys1/hotdog-sample-browser",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
